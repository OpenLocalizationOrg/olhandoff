        <unit id="143">
          <originalData>
            <data id="id1">&lt;strong&gt;</data>
            <data id="id2">&lt;/strong&gt;</data>
          </originalData>
          <segment state="initial" id="143">
            <source>
              <pc dataRefEnd="id2" dataRefStart="id1" id="1">Paused</pc>
