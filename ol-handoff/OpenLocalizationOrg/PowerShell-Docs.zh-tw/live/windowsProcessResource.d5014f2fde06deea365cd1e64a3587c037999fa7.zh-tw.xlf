        <trans-unit id="871d3f36-89f7-44c1-8492-28797b89760f" xml:space="preserve">
          <source>Arguments</source>
          <target state="new">Arguments</target>
        </trans-unit>
        <trans-unit id="e15cc3fa-a422-4a53-a153-72c4b9acb27f" xml:space="preserve">
          <source>Indicates a string of arguments to pass to the process as-is.</source>
          <target state="new">Indicates a string of arguments to pass to the process as-is.</target>
        </trans-unit>
        <trans-unit id="c097f033-129e-49a6-91d1-0f62fb06ea80" xml:space="preserve">
          <source>If you need to pass several arguments, put them all in this string.</source>
          <target state="new">If you need to pass several arguments, put them all in this string.</target>
        </trans-unit>
        <trans-unit id="b0b62241-424b-4f25-a381-f3b9d558949a" xml:space="preserve">
          <source>Path</source>
          <target state="new">Path</target>
        </trans-unit>
        <trans-unit id="df2b2a95-2b03-42e7-b68e-12bef68f42aa" xml:space="preserve">
          <source>Indicates the path to the process executable.</source>
          <target state="new">Indicates the path to the process executable.</target>
        </trans-unit>
        <trans-unit id="a2c8dd49-a7b2-4bac-b9ec-29e2eca0b892" xml:space="preserve">
          <source>If you set this property to the name of the executable, DSC will look in the <bpt id="82af685c-4297-4614-b0bf-35336f5a1ac8">&lt;strong&gt;</bpt>Path<ept id="82af685c-4297-4614-b0bf-35336f5a1ac8">&lt;/strong&gt;</ept> variable.</source>
          <target state="new">If you set this property to the name of the executable, DSC will look in the <bpt id="82af685c-4297-4614-b0bf-35336f5a1ac8">&lt;strong&gt;</bpt>Path<ept id="82af685c-4297-4614-b0bf-35336f5a1ac8">&lt;/strong&gt;</ept> variable.</target>
        </trans-unit>
        <trans-unit id="9c273638-59d0-464f-9440-e5b12d9c784e" xml:space="preserve">
          <source>If you give a fully qualified domain name, the process must exist there because DSC will not check the <bpt id="bb8510dd-a9bc-4e10-a87c-a9d038226413">&lt;strong&gt;</bpt>Path<ept id="bb8510dd-a9bc-4e10-a87c-a9d038226413">&lt;/strong&gt;</ept> variable in this case.</source>
          <target state="new">If you give a fully qualified domain name, the process must exist there because DSC will not check the <bpt id="bb8510dd-a9bc-4e10-a87c-a9d038226413">&lt;strong&gt;</bpt>Path<ept id="bb8510dd-a9bc-4e10-a87c-a9d038226413">&lt;/strong&gt;</ept> variable in this case.</target>
        </trans-unit>
        <trans-unit id="6dadaf7f-1797-4bcd-b907-49a16a7a75cf" xml:space="preserve">
