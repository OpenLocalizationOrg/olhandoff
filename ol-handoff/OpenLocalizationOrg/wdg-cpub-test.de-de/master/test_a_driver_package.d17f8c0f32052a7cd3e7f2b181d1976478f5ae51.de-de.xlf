            <data id="id1">&lt;strong&gt;</data>
            <data id="id2">&lt;/strong&gt;</data>
          </originalData>
          <segment state="initial" id="118">
            <source>Select the test computer you have configured as the <pc dataRefEnd="id2" dataRefStart="id1" id="2">Remote Computer</pc>.</source>
          </segment>
        </unit>
