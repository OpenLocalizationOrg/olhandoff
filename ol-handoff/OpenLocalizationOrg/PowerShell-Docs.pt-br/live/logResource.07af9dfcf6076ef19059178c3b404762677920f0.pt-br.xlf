        <trans-unit id="9a9441e9-2ac9-453f-afaa-5290fa2b16ea" xml:space="preserve">
          <source>DSC Log Resource</source>
          <target state="new">DSC Log Resource</target>
        </trans-unit>
        <trans-unit id="98e134bb-b04f-47fb-8965-a19e52874113" xml:space="preserve">
          <source>Applies To: Windows PowerShell 4.0, Windows PowerShell 5.0</source>
