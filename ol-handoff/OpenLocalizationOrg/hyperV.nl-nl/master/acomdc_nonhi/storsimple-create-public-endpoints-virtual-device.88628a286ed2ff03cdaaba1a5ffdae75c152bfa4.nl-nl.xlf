      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>To create public endpoints on the virtual device</source>
          <target state="new">To create public endpoints on the virtual device</target>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>Sign in to the Management Portal.</source>
          <target state="new">Sign in to the Management Portal.</target>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>Click <bpt id="p1">**</bpt>Virtual Machines<ept id="p1">**</ept>, and then select the virtual machine that is being used as your virtual device.</source>
          <target state="new">Click <bpt id="p1">**</bpt>Virtual Machines<ept id="p1">**</ept>, and then select the virtual machine that is being used as your virtual device.</target>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>Click <bpt id="p1">**</bpt>Endpoints<ept id="p1">**</ept>.</source>
          <target state="new">Click <bpt id="p1">**</bpt>Endpoints<ept id="p1">**</ept>.</target>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>The Endpoints page lists all endpoints for the virtual machine.</source>
          <target state="new">The Endpoints page lists all endpoints for the virtual machine.</target>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>Click <bpt id="p1">**</bpt>Add<ept id="p1">**</ept>.</source>
          <target state="new">Click <bpt id="p1">**</bpt>Add<ept id="p1">**</ept>.</target>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>The Add Endpoint dialog box appears.</source>
          <target state="new">The Add Endpoint dialog box appears.</target>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>Click the arrow to continue.</source>
          <target state="new">Click the arrow to continue.</target>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>For the <bpt id="p1">**</bpt>Name<ept id="p1">**</ept>, type the following name for the endpoint: <bpt id="p2">**</bpt>WinRMHttps<ept id="p2">**</ept>.</source>
          <target state="new">For the <bpt id="p1">**</bpt>Name<ept id="p1">**</ept>, type the following name for the endpoint: <bpt id="p2">**</bpt>WinRMHttps<ept id="p2">**</ept>.</target>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>For the <bpt id="p1">**</bpt>Protocol<ept id="p1">**</ept>, specify <bpt id="p2">**</bpt>TCP<ept id="p2">**</ept>.</source>
          <target state="new">For the <bpt id="p1">**</bpt>Protocol<ept id="p1">**</ept>, specify <bpt id="p2">**</bpt>TCP<ept id="p2">**</ept>.</target>
        </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>For the <bpt id="p1">**</bpt>Public Port<ept id="p1">**</ept>, type the port numbers that you want to use for the connection.</source>
          <target state="new">For the <bpt id="p1">**</bpt>Public Port<ept id="p1">**</ept>, type the port numbers that you want to use for the connection.</target>
        </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>For the <bpt id="p1">**</bpt>Private Port<ept id="p1">**</ept>, type <bpt id="p2">**</bpt>5986<ept id="p2">**</ept>.</source>
          <target state="new">For the <bpt id="p1">**</bpt>Private Port<ept id="p1">**</ept>, type <bpt id="p2">**</bpt>5986<ept id="p2">**</ept>.</target>
        </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve">
          <source>Click the check mark to create the endpoint.</source>
          <target state="new">Click the check mark to create the endpoint.</target>
        </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve">
          <source>After the endpoint is created, you can view its details to determine the Public Virtual IP (VIP) address.</source>
          <target state="new">After the endpoint is created, you can view its details to determine the Public Virtual IP (VIP) address.</target>
        </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve">
          <source>Record this address.</source>
          <target state="new">Record this address.</target>
        </trans-unit>
