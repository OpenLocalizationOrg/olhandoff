        <trans-unit id="aa24b874-18c8-4a95-a487-53ca3e4daa46" xml:space="preserve">
          <source>DSC Log Resource</source>
          <target state="new">DSC Log Resource</target>
        </trans-unit>
        <trans-unit id="05a79d56-505a-493d-8755-19d1b34c339a" xml:space="preserve">
          <source>Applies To: Windows PowerShell 4.0, Windows PowerShell 5.0</source>
