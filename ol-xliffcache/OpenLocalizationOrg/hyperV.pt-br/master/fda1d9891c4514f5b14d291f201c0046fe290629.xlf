      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve">
          <source>Configure a Network Security Group inbound rule for the VM</source>
          <target state="new">Configure a Network Security Group inbound rule for the VM</target>
        </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve">
          <source>If you want to be able to connect to SQL Server over the internet, you have to configure an inbound rule on the Network Security Group for the port that your SQL Server instance is listening.</source>
          <target state="new">If you want to be able to connect to SQL Server over the internet, you have to configure an inbound rule on the Network Security Group for the port that your SQL Server instance is listening.</target>
        </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve">
          <source>By default, this is TCP port 1433.</source>
          <target state="new">By default, this is TCP port 1433.</target>
        </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve">
          <source>In the portal, select <bpt id="p1">**</bpt>Virtual machines<ept id="p1">**</ept>, and then select your SQL Server VM.</source>
          <target state="new">In the portal, select <bpt id="p1">**</bpt>Virtual machines<ept id="p1">**</ept>, and then select your SQL Server VM.</target>
        </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve">
          <source>Click the <bpt id="p1">**</bpt>All settings<ept id="p1">**</ept> link.</source>
          <target state="new">Click the <bpt id="p1">**</bpt>All settings<ept id="p1">**</ept> link.</target>
        </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve">
          <source>Expand <bpt id="p1">**</bpt>Nework interfaces<ept id="p1">**</ept>.</source>
          <target state="new">Expand <bpt id="p1">**</bpt>Nework interfaces<ept id="p1">**</ept>.</target>
        </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve">
          <source>network interface</source>
          <target state="new">network interface</target>
        </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve">
          <source>Then select the Network Interface for your VM.</source>
          <target state="new">Then select the Network Interface for your VM.</target>
        </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve">
          <source>Click the <bpt id="p1">**</bpt>Network security group<ept id="p1">**</ept> link.</source>
          <target state="new">Click the <bpt id="p1">**</bpt>Network security group<ept id="p1">**</ept> link.</target>
        </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve">
          <source>network interface</source>
          <target state="new">network interface</target>
        </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve">
          <source>In the properties of the Network Security Group, expand <bpt id="p1">**</bpt>Inbound security rules<ept id="p1">**</ept>.</source>
          <target state="new">In the properties of the Network Security Group, expand <bpt id="p1">**</bpt>Inbound security rules<ept id="p1">**</ept>.</target>
        </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve">
          <source>Click the <bpt id="p1">**</bpt>Add<ept id="p1">**</ept> button.</source>
          <target state="new">Click the <bpt id="p1">**</bpt>Add<ept id="p1">**</ept> button.</target>
        </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve">
          <source>Provide a <bpt id="p1">**</bpt>Name<ept id="p1">**</ept> of "SQLServerPublicTraffic".</source>
          <target state="new">Provide a <bpt id="p1">**</bpt>Name<ept id="p1">**</ept> of "SQLServerPublicTraffic".</target>
        </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve">
          <source>Change <bpt id="p1">**</bpt>Protocol<ept id="p1">**</ept> to <bpt id="p2">**</bpt>TCP<ept id="p2">**</ept>.</source>
          <target state="new">Change <bpt id="p1">**</bpt>Protocol<ept id="p1">**</ept> to <bpt id="p2">**</bpt>TCP<ept id="p2">**</ept>.</target>
        </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve">
          <source>Specify a <bpt id="p1">**</bpt>Destination port range<ept id="p1">**</ept> of 1433 (or the port that your SQL Server Instance is listening on).</source>
          <target state="new">Specify a <bpt id="p1">**</bpt>Destination port range<ept id="p1">**</ept> of 1433 (or the port that your SQL Server Instance is listening on).</target>
        </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve">
          <source>Verify that <bpt id="p1">**</bpt>Action<ept id="p1">**</ept> is set to <bpt id="p2">**</bpt>Allow<ept id="p2">**</ept>.</source>
          <target state="new">Verify that <bpt id="p1">**</bpt>Action<ept id="p1">**</ept> is set to <bpt id="p2">**</bpt>Allow<ept id="p2">**</ept>.</target>
        </trans-unit>
