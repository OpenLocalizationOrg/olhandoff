        <trans-unit id="119" translate="yes" xml:space="preserve">
          <source>Click <bpt id="p1">**</bpt>OK<ept id="p1">**</ept> to save the rule for your VM.</source>
          <target state="new">Click <bpt id="p1">**</bpt>OK<ept id="p1">**</ept> to save the rule for your VM.</target>
