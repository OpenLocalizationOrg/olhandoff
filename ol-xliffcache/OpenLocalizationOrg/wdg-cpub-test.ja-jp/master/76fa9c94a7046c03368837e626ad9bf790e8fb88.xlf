            <data id="id2">&lt;/em&gt;</data>
          </originalData>
          <segment state="initial" id="104">
            <source xml:space="preserve">The target computer is also called the <pc dataRefEnd="id2" dataRefStart="id1" id="2">test computer</pc>.</source>
            <target xml:space="preserve">The target computer is also called the <pc dataRefEnd="id2" dataRefStart="id1" id="2">test computer</pc>.</target>
          </segment>
        </unit>
        <unit id="105">
          <segment state="initial" id="105">
            <source xml:space="preserve">You can debug a user-mode driver on the host computer or on a separate target computer.</source>
            <target xml:space="preserve">You can debug a user-mode driver on the host computer or on a separate target computer.</target>
          </segment>
        </unit>
        <unit id="106">
