          <originalData>
            <data id="id1">&lt;strong&gt;</data>
            <data id="id2">&lt;/strong&gt;</data>
          </originalData>
          <segment state="initial" id="119">
            <source xml:space="preserve">| <pc dataRefEnd="id2" dataRefStart="id1" id="2">Unreliable</pc>  | There is currently a high degree of inaccuracy in the magnetometer.</source>
            <target xml:space="preserve">| <pc dataRefEnd="id2" dataRefStart="id1" id="2">Unreliable</pc>  | There is currently a high degree of inaccuracy in the magnetometer.</target>
          </segment>
        </unit>
        <unit id="120">
          <segment state="initial" id="120">
            <source xml:space="preserve">Apps should always ask for a calibration from the user when this value is first returned.</source>
            <target xml:space="preserve">Apps should always ask for a calibration from the user when this value is first returned.</target>
          </segment>
        </unit>
