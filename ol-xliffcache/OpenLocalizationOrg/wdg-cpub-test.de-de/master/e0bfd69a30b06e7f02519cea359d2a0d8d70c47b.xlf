      <group id="101">
        <unit id="101">
          <segment state="initial" id="101">
            <source>KMDF Verifier Properties for Driver Package Projects</source>
          </segment>
        </unit>
        <unit id="102">
          <segment state="initial" id="102">
            <source>Sets the properties for the KMDF Verifier (or framework verifier) on a remote computer.</source>
          </segment>
        </unit>
        <unit id="103">
          <segment state="initial" id="103">
            <source>You can use these settings when you build and deploy a KMDF driver to a test computer.</source>
          </segment>
        </unit>
        <unit id="104">
          <originalData>
            <data id="id1">&lt;linkText&gt;</data>
            <data id="id2">&lt;/linkText&gt;</data>
            <data id="id3">&lt;title&gt;</data>
            <data id="id4">&lt;/title&gt;</data>
            <data id="id5">&lt;link&gt;</data>
            <data id="id6">&lt;/link&gt;</data>
          </originalData>
          <segment state="initial" id="104">
            <source>For information about KMDF drivers, see <pc dataRefEnd="id2" dataRefStart="id1" id="2CapsExtId2">Kernel-Mode Driver Framework</pc><pc dataRefEnd="id4" dataRefStart="id3" id="2CapsExtId3"></pc><pc dataRefEnd="id6" dataRefStart="id5" id="2CapsExtId1"></pc>.</source>
          </segment>
        </unit>
        <unit id="105">
          <originalData>
            <data id="id1">&lt;linkText&gt;</data>
