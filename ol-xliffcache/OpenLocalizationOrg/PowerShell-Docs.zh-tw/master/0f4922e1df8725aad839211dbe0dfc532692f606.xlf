        <trans-unit id="a4c0f634-ae3d-4034-9949-9c73b6ad90e2" xml:space="preserve">
          <source>DSC Log Resource</source>
          <target state="new">DSC Log Resource</target>
        </trans-unit>
        <trans-unit id="621d7594-93c9-4845-8cf0-659981a07775" xml:space="preserve">
          <source>Applies To: Windows PowerShell 4.0, Windows PowerShell 5.0</source>
